<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/styleguide.css" />
    <link rel="stylesheet" href="/css/globals.css" />
    <link rel="stylesheet" href="/css/home.css" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <!-- NAVBAR -->
    <%- include('partials/navbar', { activePage: 'home' }) %>
    <!-- HOME SLIDER -->
    <div
      id="carouselExampleInterval"
      class="carousel slide"
      data-bs-ride="carousel"
    >
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img
            src="/img/home_screen/background-of-school-1.png"
            class="d-block w-100"
            style="height: 672px"
            alt="slide-1"
          />
        </div>
        <div class="carousel-item">
          <img
            src="/img/home_screen/background-of-school-2.png"
            class="d-block w-100"
            style="height: 672px"
            alt="slide-2"
          />
        </div>
        <div class="carousel-item">
          <img
            src="/img/home_screen/background-of-school-3.png"
            class="d-block w-100"
            style="height: 672px"
            alt="slide-3"
          />
        </div>
      </div>
      <button
        class="carousel-control-prev"
        type="button"
        data-bs-target="#carouselExampleInterval"
        data-bs-slide="prev"
      >
        <span
          class="carousel-control-prev-icon visually-hidden"
          aria-hidden="true"
        ></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button
        class="carousel-control-next"
        type="button"
        data-bs-target="#carouselExampleInterval"
        data-bs-slide="next"
      >
        <span
          class="carousel-control-next-icon visually-hidden"
          aria-hidden="true"
        ></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
    <!-- LATEST DEADLINE -->
    <div class="deadline">
      <div
        class="course-card"
        onclick="window.location.href='http:\/\/localhost:3000';"
      >
        <!-- title -->
        <div class="title text-wrapper-12">Introduction to Programming</div>
        <div class="d-flex justify-content-between align-items-center w-100">
          <div class="text-wrapper-13">Time remaining</div>
          <div class="text-wrapper-14">Final Project</div>
        </div>
        <div class="text-wrapper-15">3 days 23 hours</div>
      </div>
      <div
        class="course-card"
        onclick="window.location.href='http:\/\/localhost:3000';"
      >
        <!-- title -->
        <div class="title text-wrapper-12 w-100">
          Introduction to Software Engineering
        </div>
        <div class="d-flex justify-content-between align-items-center w-100">
          <div class="text-wrapper-13">Time remaining</div>
          <div class="text-wrapper-14">Final Project</div>
        </div>
        <div class="text-wrapper-15">3 days 23 hours</div>
      </div>
      <div
        class="course-card"
        onclick="window.location.href='http:\/\/localhost:3000';"
      >
        <!-- title -->
        <div class="title text-wrapper-12">Introduction to Data Science</div>
        <div class="d-flex justify-content-between align-items-center w-100">
          <div class="text-wrapper-13">Time remaining</div>
          <div class="text-wrapper-14">Final Project</div>
        </div>
        <div class="text-wrapper-15">3 days 23 hours</div>
      </div>
    </div>

    <!-- MAIN BODY -->
    <div id="main">
      <!-- CURRENT SEMESTER TITLE -->
      <div class="text-wrapper-7 mb-5">Semester 1 - Junior</div>

      <!-- COURSES -->
      <div class="course-card mx-auto mb-5" style="width: 90%">
        <!-- KEY-ICON -->
        <img
          src="/img/home_screen/solar-key-linear.svg"
          alt="key-icon"
          style="
            position: absolute;
            top: 0;
            bottom: 0;
            right: 57px;
            margin: auto;
          "
        />
        <!-- COURSE TITLE -->
        <div class="title text-wrapper-8">
          Introduction to Machine Learning - CSC14005
        </div>
        <!-- COURSE CLASS -->
        <div class="text-wrapper-9 mb-3">21KHMT01</div>
        <!-- COURSE TEACHERS -->
        <div class="text-wrapper-9">
          <span>Teacher: </span>
          <span style="color: #ff7c00">Bui Tien Len</span>
        </div>
        <div class="text-wrapper-9">
          <span>Teacher: </span>
          <span style="color: #ff7c00">Le Nhut Nam</span>
        </div>
        <div class="text-wrapper-9">
          <span>Teacher: </span>
          <span style="color: #ff7c00">Nguyen Ngoc Duc</span>
        </div>
      </div>

      <div class="course-card mx-auto mb-5" style="width: 90%">
        <!-- KEY-ICON -->
        <img
          src="/img/home_screen/solar-key-linear.svg"
          alt="key-icon"
          style="
            position: absolute;
            top: 0;
            bottom: 0;
            right: 57px;
            margin: auto;
          "
        />
        <!-- COURSE TITLE -->
        <div class="title text-wrapper-8">
          Introduction to Machine Learning - CSC14005
        </div>
        <!-- COURSE CLASS -->
        <div class="text-wrapper-9 mb-3">21KHMT01</div>
        <!-- COURSE TEACHERS -->
        <div class="text-wrapper-9">
          <span>Teacher: </span>
          <span style="color: #ff7c00">Bui Tien Len</span>
        </div>
        <div class="text-wrapper-9">
          <span>Teacher: </span>
          <span style="color: #ff7c00">Le Nhut Nam</span>
        </div>
        <div class="text-wrapper-9">
          <span>Teacher: </span>
          <span style="color: #ff7c00">Nguyen Ngoc Duc</span>
        </div>
      </div>
    </div>

    <!-- FOOTER -->
    <%- include('partials/footer') %>
  </body>
</html>
